      <key>favfile</key>
      <type>upload</type>
      <filetype>image</filetype>
      <uploadmodule>fav</uploadmodule>
    </field>
    <field>
      <name>联系人姓名</name>
      <key>contacter</key>
      <type>text</type>
    </field>
    <field>
      <name>QQ</name>
      <key>qq</key>
      <type>text</type>
    </field>
    <field>
      <name>邮箱</name>
      <key>email</key>
      <type>text</type>
    </field>
    <field>
      <name>手机号码</name>
      <key>mobile</key>
      <type>text</type>
    </field>
    <field>
      <name>电话</name>
      <key>phone</key>
      <type>text</type>
    </field>
    <field>
      <name>Seo 标题</name>
      <key>seo_title</key>
      <type>longtext</type>
    </field>
    <field>
      <name>Seo 关键字</name>
      <key>seo_keywords</key>
